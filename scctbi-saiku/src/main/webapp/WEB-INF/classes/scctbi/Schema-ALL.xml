<Schema name="Delivery Order">

	<Dimension name="Status">
		<Hierarchy hasAll="true" allMemberName="All Status" primaryKey="statusCode">
			<Table name="orderStatus" />
			<Level name="Status" column="statusCode" ordinalColumn="milestoneCode" uniqueMembers="true" />
		</Hierarchy>
	</Dimension>

	<Dimension name="deliveryOrder">
		<Hierarchy hasAll="true" allMemberName="All deliveryOrder" primaryKey="dlvryNum">
			<Table name="deliveryOrder" />
			<Level name="DeliveryNumber" column="dlvryNum">
				<Property name="Carrier Code" column="carrCode" />
			</Level>
		</Hierarchy>
	</Dimension>
	
	<Dimension name="PreCarrier" caption="1st Leg Carrier">
		<Hierarchy hasAll="true" allMemberName="All PreCarriers" primaryKeyTable="deliveryOrder" primaryKey="dlvryNum">
			<Join leftKey="preCarrier" rightKey="carrCode">
				<Table name="deliveryOrder" />
				<Table name="carrier" />
			</Join>
			<Level name="PreCarrier Code" table="carrier" column="carrCode" uniqueMembers="true" caption="1st Leg Carrier Code"/>
			<Level name="PreCarrier Name" table="carrier" column="carrName" caption="1st Leg Carrier Name"/>
		</Hierarchy>
	</Dimension>

	<Dimension name="Carrier" caption="2nd Leg Carrier">
		<Hierarchy hasAll="true" allMemberName="All Carriers" primaryKey="carrCode">
			<Table name="carrier" />
			<Level name="Carrier Code" column="carrCode" uniqueMembers="true" caption="2nd Leg Carrier Code"/>
			<Level name="Carrier Name" column="carrName" caption="2nd Leg Carrier Name"/>
		</Hierarchy>
	</Dimension>

	<Dimension name="MOT">
		<Hierarchy hasAll="true" allMemberName="All MOT" primaryKey="motCode">
			<Table name="mot" />
			<Level name="MOT" column="motName" uniqueMembers="true" />
		</Hierarchy>
	</Dimension>

	<Dimension name="ShipToCountry">
		<Hierarchy hasAll="true" allMemberName="All Countries" primaryKey="ctryCode">
			<Table name="country" />
			<Level name="Region" column="ctryRegion" uniqueMembers="true" />
			<Level name="SubGeo" column="subGeo" uniqueMembers="true" />
			<Level name="Country" nameColumn="ctryName" column="ctryCode" uniqueMembers="true" />
		</Hierarchy>
	</Dimension>

	<Dimension name="ServiceLevel">
		<Hierarchy hasAll="true" allMemberName="All ServiceLevels" primaryKey="dlvryNum">
			<Table name="deliveryOrder" />
			<Level name="ServiceLevel" column="serviceLevel" />
		</Hierarchy>
	</Dimension>

	<Dimension name="ShippingPoint">
		<Hierarchy hasAll="true" allMemberName="All ShippingPoints" primaryKey="shpPointCode">
			<Table name="shpPoint" />
			<Level name="ShippingPoint" column="shpPointCode" nameColumn="shpPointName" uniqueMembers="true" />
		</Hierarchy>
	</Dimension>

	<Dimension name="OrderType">
		<Hierarchy hasAll="true" allMemberName="All OrderTypes" primaryKey="dlvryNum">
			<Table name="deliveryOrder" />
			<Level name="OrderType" column="orderType" />
		</Hierarchy>
	</Dimension>
	

	<!-- KPI -->

	<Dimension name="KPI-CDD" caption="KPI: CDD">
		<Hierarchy hasAll="true" allMemberName="All KPI-CDD" primaryKeyTable="deliveryOrder" primaryKey="dlvryNum">
			<Join leftKey="dk01Color" rightKey="code">
				<Table name="deliveryOrder" />
				<Table name="kpiColor" />
			</Join>
			<Level name="CDD" table="kpiColor" column="code" nameColumn="name" />
		</Hierarchy>
	</Dimension>

	<Dimension name="KPI-IOD" caption="KPI: IOD">
		<Hierarchy hasAll="true" allMemberName="All KPI-IOD" primaryKeyTable="deliveryOrder" primaryKey="dlvryNum">
			<Join leftKey="dk02Color" rightKey="code">
				<Table name="deliveryOrder" />
				<Table name="kpiColor" />
			</Join>
			<Level name="IOD" table="kpiColor" column="code" nameColumn="name" />
		</Hierarchy>
	</Dimension>

	<Dimension name="KPI-M3.5" caption="KPI: M3.5">
		<Hierarchy hasAll="true" allMemberName="All KPI-M3.5" primaryKeyTable="deliveryOrder" primaryKey="dlvryNum">
			<Join leftKey="dk03Color" rightKey="code">
				<Table name="deliveryOrder" />
				<Table name="kpiColor" />
			</Join>
			<Level name="M3.5" table="kpiColor" column="code" nameColumn="name" />
		</Hierarchy>
	</Dimension>

	<Dimension name="KPI-M4.5" caption="KPI: M4.5">
		<Hierarchy hasAll="true" allMemberName="All KPI-M4.5" primaryKeyTable="deliveryOrder" primaryKey="dlvryNum">
			<Join leftKey="dk04Color" rightKey="code">
				<Table name="deliveryOrder" />
				<Table name="kpiColor" />
			</Join>
			<Level name="M4.5" table="kpiColor" column="code" nameColumn="name" />
		</Hierarchy>
	</Dimension>

	<Dimension name="KPI-TOTH" caption="KPI: TOTH">
		<Hierarchy hasAll="true" allMemberName="All KPI-TOTH" primaryKeyTable="deliveryOrder" primaryKey="dlvryNum">
			<Join leftKey="dk05Color" rightKey="code">
				<Table name="deliveryOrder" />
				<Table name="kpiColor" />
			</Join>
			<Level name="TOTH" table="kpiColor" column="code" nameColumn="name" />
		</Hierarchy>
	</Dimension>

