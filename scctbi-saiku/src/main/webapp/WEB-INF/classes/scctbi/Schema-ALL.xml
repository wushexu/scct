			<CalculatedMemberProperty name="FORMAT_STRING" value="0.00%" />
		</CalculatedMember>

		<CalculatedMember name="KPI-M3.5 %" dimension="Measures">
			<Formula>([Measures].[Count])/([Measures].[Count],[All KPI-M3.5])</Formula>
			<CalculatedMemberProperty name="FORMAT_STRING" value="0.00%" />
		</CalculatedMember>

		<CalculatedMember name="KPI-M4.5 %" dimension="Measures">
			<Formula>([Measures].[Count])/([Measures].[Count],[All KPI-M4.5])</Formula>
			<CalculatedMemberProperty name="FORMAT_STRING" value="0.00%" />
		</CalculatedMember>

		<CalculatedMember name="KPI-TOTH %" dimension="Measures">
			<Formula>([Measures].[Count])/([Measures].[Count],[All KPI-TOTH])</Formula>
			<CalculatedMemberProperty name="FORMAT_STRING" value="0.00%" />
		</CalculatedMember>

	</Cube>
	
