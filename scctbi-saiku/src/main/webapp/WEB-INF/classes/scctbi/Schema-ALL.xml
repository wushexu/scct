		<!-- <Measure name="Item Count" column="dlvryItemQty" aggregator="sum" formatString="#,#" /> -->

		<CalculatedMember name="Status %" dimension="Measures">
			<Formula>([Measures].[Count])/([Measures].[Count],[All Status])</Formula>
			<CalculatedMemberProperty name="FORMAT_STRING" value="0.00%" />
		</CalculatedMember>

		<CalculatedMember name="Country %" dimension="Measures">
			<Formula>([Measures].[Count])/([Measures].[Count],[All Countries])</Formula>
